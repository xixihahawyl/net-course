        function SetInputFocus() {
            setTimeout(function () {
                $("input.input_index").focus();
            }, 200);
        }

        $().ready(function () {
            SetInputFocus();
        });

        $(function () {
            var searchInput = $("#w");
            var v = searchInput.val();
            searchInput.val("");
            searchInput.focus();
            searchInput.val(v);


        })
    </script>
